{% extends 'base.html.twig' %}

{% block title %}
    {{ page.title }} - {{ parent() }}
{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-sm-16 col-xs-24">
            <h1>{{ page.title }}</h1>
            {{ page.content|raw }}
        </div>
        <div class="col-sm-8 col-xs-24">
            {% include 'default/block.html.twig' %}
        </div>
    </div>
{% endblock %}
