{% extends 'base.html.twig' %}

{% block title %}
    {{ page.title }} - {{ parent() }}
{% endblock %}

{% block body %}
    <section id="content">
        <div class="container_24">
            <div class="wrapper">
                <article class="grid_24">
                    <h1>{{ page.title }}</h1>
                    {{ page.content|raw }}
                </article>
            </div>
        </div>
    </section>
{% endblock %}
