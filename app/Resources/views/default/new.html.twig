{% extends 'base.html.twig' %}

{% block title %}
    {{ news.title }} - {{ parent() }}
{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-sm-16 col-xs-24">
            <h1>{{ news.title }}</h1>
            <p>{{ news.content|raw }}</p>
        </div>
        <div class="col-sm-8 col-xs-24">
            {% include 'default/block.html.twig' %}
        </div>
    </div>
{% endblock %}
